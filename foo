
func removeExecParams(execCmd string) string {
	regexPattern := regexp.MustCompile(`%\w`)
	parts := strings.Split(execCmd, " ")
	var cleanedParts []string
	for _, part := range parts {
		if !regexPattern.MatchString(part) {
			cleanedParts = append(cleanedParts, part)
		}
	}
	cleanedCmd := strings.Join(cleanedParts, " ")
	return cleanedCmd
}
